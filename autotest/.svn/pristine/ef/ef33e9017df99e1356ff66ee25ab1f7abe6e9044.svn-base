package com.activenetwork.qa.awo.datacollection.legacy;

/**
 * @author jchen
 *
 * TODO To change the template for this generated type comment go to
 * Window - Preferences - Java - Code Style - Code Templates
 */
public class LiveData {
	// variables only for UWP
	private String email;

	// variables only for ORMS
	private String fmUser;

	private String fmPw;

	private String fmPin;

	private String cmUser;

	private String cmPw;

	private String omUser;

	private String omPw;

	// common variables
	private String payType = "VISA";

	private String cardNum;

	private String expiryMonth;

	private String expiryYear;

	private String firstName;

	private String lastName;

	private String testerName;

	private boolean dataSet;

	/**
	 * @return Returns the email.
	 */
	public String getEmail() {
		return email;
	}

	/**
	 * @param email The email to set.
	 */
	public void setEmail(String email) {
		this.email = email;
	}

	/**
	 * @return Returns the field manager user.
	 */
	public String getFmUser() {
		return fmUser;
	}

	/**
	 * @param fmUser The fmUser to set.
	 */
	public void setFmUser(String fmUser) {
		this.fmUser = fmUser;
	}

	/**
	 * @return Returns the field manager user password.
	 */
	public String getFmPw() {
		return fmPw;
	}

	/**
	 * @param fmPw The fmPw to set.
	 */
	public void setFmPw(String fmPw) {
		this.fmPw = fmPw;
	}

	/**
	 * @return pin number for Field Manager user
	 */
	public String getFmPin() {
		return fmPin;
	}

	/**
	 * @param fmPin the pin number to set
	 */
	public void setFmPin(String fmPin) {
		this.fmPin = fmPin;
	}

	/**
	 * @return Returns the call manager user.
	 */
	public String getCmUser() {
		return cmUser;
	}

	/**
	 * @param cmUser The cmUser to set.
	 */
	public void setCmUser(String cmUser) {
		this.cmUser = cmUser;
	}

	/**
	 * @return Returns the call manager user password.
	 */
	public String getCmPw() {
		return cmPw;
	}

	/**
	 * @param cmPw The cmPw to set.
	 */
	public void setCmPw(String cmPw) {
		this.cmPw = cmPw;
	}

	/**
	 * @return Returns the operation manager user.
	 */
	public String getOmUser() {
		return omUser;
	}

	/**
	 * @param omUser The omUser to set.
	 */
	public void setOmUser(String omUser) {
		this.omUser = omUser;
	}

	/**
	 * @return Returns the operation manager user password.
	 */
	public String getOmPw() {
		return omPw;
	}

	/**
	 * @param omPw The omPw to set.
	 */
	public void setOmPw(String omPw) {
		this.omPw = omPw;
	}

	/**
	 * @return Returns the expiryMonth.
	 */
	public String getExpiryMonth() {
		return expiryMonth;
	}

	/**
	 * @param expiryMonth The expiryMonth to set.
	 */
	public void setExpiryMonth(String expiryMonth) {
		this.expiryMonth = expiryMonth;
	}

	/**
	 * @return Returns the firstName.
	 */
	public String getFirstName() {
		return firstName;
	}

	/**
	 * @param firstName The firstName to set.
	 */
	public void setFirstName(String firstName) {
		this.firstName = firstName;
	}

	/**
	 * @return Returns the lastName.
	 */
	public String getLastName() {
		return lastName;
	}

	/**
	 * @param lastName The lastName to set.
	 */
	public void setLastName(String lastName) {
		this.lastName = lastName;
	}

	/**
	 * @return Returns the payType.
	 */
	public String getPayType() {
		return payType;
	}

	/**
	 * @param payType The payType to set.
	 */
	public void setPayType(String payType) {
		this.payType = payType;
	}

	/**
	 * @return Returns the cardNum.
	 */
	public String getCardNum() {
		return cardNum;
	}

	/**
	 * @param cardNum The cardNum to set.
	 */
	public void setCardNum(String cardNum) {
		this.cardNum = cardNum;
	}

	/**
	 * @return Returns the expiryYear.
	 */
	public String getExpiryYear() {
		return expiryYear;
	}

	/**
	 * @param expiryYear The expiryYear to set.
	 */
	public void setExpiryYear(String expiryYear) {
		this.expiryYear = expiryYear;
	}

	/**
	 * @return Returns the dataSet.
	 */
	public boolean isDataSet() {
		return dataSet;
	}

	/**
	 * @param dataSet The dataSet to set.
	 */
	public void setDataSet(boolean dataSet) {
		this.dataSet = dataSet;
	}

	public String toString() {
		return payType + " " + cardNum + " " + expiryMonth + " " + expiryYear
				+ " " + firstName + " " + lastName + " " + dataSet;

	}

	/**
	 * @return Returns the testerName.
	 */
	public String getTesterName() {
		return testerName;
	}

	/**
	 * @param testerName The testerName to set.
	 */
	public void setTesterName(String testerName) {
		this.testerName = testerName;
	}
}
