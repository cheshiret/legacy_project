package com.activenetwork.qa.awo.datacollection.legacy.orms;

import java.util.ArrayList;
import java.util.List;

/**
 * This data class is used for store all paras for ORMS order summary page for various products, 
 * 		campsite, ticket, permit, etc.
 */
public class OrderSummaryInfo {
	
	// Receipt Details section
	public String receiptNum = "";
	public String receiptDateTime = "";
	public String salesLocation = "";
	public String createdBy = "";
	
	// Customer section
	public String custName = "";
	public String custEmail = "";
	public String custPhone = "";
	public String custOrgName = "";
	
	// Transaction(s) section
	public List<String> transactions = new ArrayList<String>();
	
	// Receipt Item section
	public String orderNum = "";
	public String[] orderNums = new String[]{};
	public String facilityName = "";
	public String prdCategory = "";
	public String siteNum = "";
	public List<String> siteNums = new ArrayList<String>();
	public String arrivalDate = "";
	public String[] arrivalDates = new String[]{};
	//------Ticket
	public String tourName = "";
	public List<String> tourNames = new ArrayList<String>();
	public String numOfTickets = "";
	
	
	//Totals section
	public String totalPrice = "";
	public String totalPastPaid = "";
	
	//Payments & Change section
	public String paymentTendered = "";
	public String changeTendered = "";
	
	//Outstanding section
	public String amountOwing = "";
	
	//Payment section
	public String paymentType = "";
	public String amount = "";
	public String cardNum = "";
	public String expiryDate = "";
	public String cardHolder = "";
	public String currency = "";
		
}
